#!/usr/bin/ctr

json := JSON new.

json put: 'Moo' at: 'Cow'.
json put: 'Meow' at: 'Cat'.
json put: 'Ruff' at: 'Dog'.

speak := { :animal ^ json at: animal. }.

catcall := speak applyTo: 'Cat'.
cowcall := speak applyTo: 'Cow'.

Pen write: 'Printing calls...'.

Pen write: catcall.
Pen write: cowcall.
Pen write: '\n'.

json delete.
